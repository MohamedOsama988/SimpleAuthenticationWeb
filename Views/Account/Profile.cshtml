@model ApplicationUser
@{
    ViewData["Title"] = "Profile";
}

<legend>Your Profile Information</legend>
<div class="form-group row">
    <label asp-for="UserName" class="col-sm-2 col-form-label">User name</label>
    <div class="col-sm-10">
        <input type="text" readonly="" class="form-control-plaintext" value="@Model.UserName" asp-for="UserName">
    </div>
</div>
<div class="form-group row">
    <label asp-for="FirstName" class="col-sm-2 col-form-label">First name</label>
    <div class="col-sm-10">
        <input type="text" readonly="" class="form-control-plaintext" value="@Model.FirstName" asp-for="FirstName">
    </div>
</div>
<div class="form-group row">
    <label asp-for="LastName" class="col-sm-2 col-form-label">Last name</label>
    <div class="col-sm-10">
        <input type="text" readonly="" class="form-control-plaintext" value="@Model.LastName" asp-for="LastName">
    </div>
</div>
<div class="form-group row">
    <label asp-for="Email" class="col-sm-2 col-form-label">Email</label>
    <div class="col-sm-10">
        <input type="text" readonly="" class="form-control-plaintext" value="@Model.Email" asp-for="Email">
    </div>
</div>
